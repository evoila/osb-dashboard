    "pree2e": "webdriver-manager update --chrome true --gecko false --standalone false --versions.chrome 2.29",
    "e2e": "run-s e2e:auto-scaler e2e:cassandra e2e:elasticsearch e2e:example e2e:lbaas e2e:monitoring e2e:mongodb e2e:mysql e2e:postgresql e2e:rabbitmq e2e:redis",
    "e2e:auto-scaler": "ts-node ./scripts/dist-e2e.ts -t auto-scaler",
    "e2e:cassandra": "ts-node ./scripts/dist-e2e.ts -t cassandra",
    "e2e:elasticsearch": "ts-node ./scripts/dist-e2e.ts -t elasticsearch",
    "e2e:example": "ts-node ./scripts/dist-e2e.ts -t example",
    "e2e:kafka": "ts-node ./scripts/dist-e2e.ts -t kafka",
    "e2e:lbaas": "ts-node ./scripts/dist-e2e.ts -t lbaas",
    "e2e:monitoring": "ts-node ./scripts/dist-e2e.ts -t monitoring",
    "e2e:mongodb": "ts-node ./scripts/dist-e2e.ts -t mongodb",
    "e2e:mysql": "ts-node ./scripts/dist-e2e.ts -t mysql",
    "e2e:postgresql": "ts-node ./scripts/dist-e2e.ts -t postgresql",
    "e2e:rabbitmq": "ts-node ./scripts/dist-e2e.ts -t rabbitmq",
    "e2e:redis": "ts-node ./scripts/dist-e2e.ts -t redis",
    "ci": "yarn run tslint && yarn run test && yarn run build && yarn run e2e",
    "ci:auto-scaler": "yarn run tslint && yarn run test && yarn run build:auto-scaler && yarn run e2e:auto-scaler",
    "ci:cassandra": "yarn run tslint && yarn run test && yarn run build:cassandra && yarn run e2e:cassandra",
    "ci:example": "yarn run tslint && yarn run test && yarn run build:example && yarn run e2e:example",
    "ci:elasticsearch": "yarn run tslint && yarn run test && yarn run build:elasticsearch && yarn run e2e:elasticsearch",
    "ci:kafka": "yarn run tslint && yarn run test && yarn run build:kafka && yarn run e2e:kafka",
    "ci:lbaas": "yarn run tslint && yarn run test && yarn run build:lbaas && yarn run e2e:lbaas",
    "ci:monitoring": "yarn run tslint && yarn run test && yarn run build:monitoring && yarn run e2e:monitoring",
    "ci:mongodb": "yarn run tslint && yarn run test && yarn run build:mongodb && yarn run e2e:mongodb",
    "ci:mysql": "yarn run tslint && yarn run test && yarn run build:mysql && yarn run e2e:mysql",
    "ci:postgresql": "yarn run tslint && yarn run test && yarn run build:postgresql && yarn run e2e:postgresql",
    "ci:rabbitmq": "yarn run tslint && yarn run test && yarn run build:rabbitmq && yarn run e2e:rabbitmq",
    "ci:redis": "yarn run tslint && yarn run test && yarn run build:redis && yarn run e2e:redis",
    "privateci": "fly -t ci execute -c ./../ci/build.yml -i repo=./../ -x",
    "postinstall": "ngcc"
  },
  "private": true,
