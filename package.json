    "start:kafka": "ts-node ./scripts/start.ts -t kafka",
    "start:lbaas": "ts-node ./scripts/start.ts -t lbaas",
    "start:mongodb": "ts-node ./scripts/start.ts -t mongodb",
    "start:monitoring": "ts-node ./scripts/start.ts -t monitoring",
