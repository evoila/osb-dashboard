    "tslint": "tslint \"src/**/*.ts\"",
    "test": "ts-node ./scripts/test.ts",
    "prebuild": "ts-node ./scripts/git-version.ts",
    "build": "run-p --max-parallel 2 build:auto-scaler build:cassandra build:elasticsearch build:example build:lbaas build:kafka build:monitoring build:mongodb build:mysql build:postgresql build:rabbitmq build:redis",
    "build:auto-scaler": "ts-node ./scripts/build.ts -t auto-scaler",
    "build:cassandra": "ts-node ./scripts/build.ts -t cassandra",
